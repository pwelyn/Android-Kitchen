#
# 此脚本有两个强制性参数:
#
# $1 = 设备名称
# $2 = edify_defs 文件中的参数
#

device=$1
param=$2

base_dir=`pwd`


if [ ! -e tools/edify_defs/$device ]
then
  value=UNKNOWN

else

  cd tools/edify_defs

  str=^$param=
  grep_str=`grep -c $param $device`

  if [ $grep_str == 0 ]
  then
    value=UNKNOWN

  else
    value=`grep -m 1 $param $device \
            | sed 's/'"$param"'=//g'`

    if [ "$value" == "" ]
    then
      value=UNKNOWN
    fi
  fi

  cd $base_dir
fi

echo $value
